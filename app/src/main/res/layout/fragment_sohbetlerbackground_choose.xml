<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Pages.SohbetlerbackgroundChooseFragment">


    <ImageView
        android:id="@+id/backgroundd"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="50dp"
        android:src="@drawable/background"
        app:layout_constraintEnd_toStartOf="@+id/background11"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/background11"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="50dp"

        android:foregroundGravity="center"
        android:src="@drawable/background1"
        app:layout_constraintEnd_toEndOf="@+id/guncelle"
        app:layout_constraintStart_toStartOf="@+id/guncelle"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/background22"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="50dp"
